{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "index.py"
        },
        {
            "src": "/parse",
            "dest": "index.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "FLASK_ENV": "production",
        "FLASK_APP": "index.py"
    }
} 
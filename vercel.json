{
    "version": 2,
    "builds": [
        {
            "src": "video_parser.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "video_parser.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "FLASK_ENV": "production",
        "FLASK_APP": "video_parser.py"
    }
} 